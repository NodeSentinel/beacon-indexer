{
  "name": "fetch",
  "version": "1.0.0",
  "description": "Collect validators information from a beacon node, process and save it to the db.",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:gnosis": "npx tsx src/index.ts",
    "dev:ethereum": "npx tsx src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "vitest run --config vitest.e2e.config.ts --no-coverage",
    "xstate-logs": "watch -n 1 -c cat logs/machines-status.log",
    "clean-db": "tsx src/utils/cleanDatabase.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@beacon-indexer/db": "workspace:*",
    "@t3-oss/env-core": "^0.10.1",
    "axios": "^1.7.2",
    "axios-cache-interceptor": "^1.8.0",
    "axios-logger": "^2.8.1",
    "axios-rate-limit": "^1.4.0",
    "bitlist": "^1.0.0",
    "bitset": "^5.1.1",
    "chalk": "^5.6.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.17",
    "ms": "^2.1.3",
    "p-limit": "^7.1.1",
    "p-retry": "^6.2.0",
    "pg": "^8.5.1",
    "pino": "^9.3.1",
    "pino-http": "^10.5.0",
    "pino-pretty": "^11.2.1",
    "pino-tee": "^0.3.0",
    "rate-limiter-flexible": "^5.0.3",
    "toad-scheduler": "^3.0.1",
    "viem": "^2.23.13",
    "xstate": "^5.22.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash": "^4.14.200",
    "@types/memoizee": "^0.4.11",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  }
}
